{
  "name": "@spark-ui/components",
  "version": "12.0.0",
  "license": "MIT",
  "description": "Spark (Leboncoin design system) components.",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./form-field": {
      "types": "./dist/form-field/index.d.ts",
      "import": "./dist/form-field/index.mjs",
      "require": "./dist/form-field/index.js"
    },
    "./*": {
      "types": "./dist/*/index.d.ts",
      "import": "./dist/*/index.mjs",
      "require": "./dist/*/index.js"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "@spark-ui",
    "react",
    "component",
    "accessible",
    "accessibility",
    "wai-aria",
    "aria",
    "a11y"
  ],
  "scripts": {
    "build": "NODE_OPTIONS='--max-old-space-size=8192' tsup"
  },
  "dependencies": {
    "@base-ui-components/react": "^1.0.0-beta.3",
    "@react-aria/button": "3.14.2",
    "@react-aria/numberfield": "3.12.2",
    "@react-aria/toast": "^3.0.0-beta.18",
    "@react-stately/numberfield": "3.10.2",
    "@react-stately/toast": "^3.0.0-beta.7",
    "@spark-ui/hooks": "^12.0.0",
    "@spark-ui/icons": "^12.0.0",
    "@spark-ui/internal-utils": "^12.0.0",
    "@zag-js/pagination": "1.29.1",
    "@zag-js/react": "1.29.1",
    "class-variance-authority": "0.7.1",
    "downshift": "9.0.10",
    "emulate-tab": "^1.2.1",
    "radix-ui": "1.4.3",
    "react-snap-carousel": "^0.5.1"
  },
  "peerDependencies": {
    "@spark-ui/theme-utils": "latest",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "tailwindcss": "^4.1.13"
  },
  "devDependencies": {
    "jsdom-testing-mocks": "1.16.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/leboncoin/spark-web.git",
    "directory": "packages/components"
  },
  "bugs": {
    "url": "https://github.com/leboncoin/spark-web/issues?q=is%3Aopen+label%3A%22Component%3A+button%22"
  },
  "homepage": "https://sparkui.vercel.app"
}
